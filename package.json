{
  "name": "nodejs-pro",
  "version": "1.0.0",
  "description": "nodejs pro",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "dev": "nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register -r tsconfig-paths/register ./src/app.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zabaoit/node-pro.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "zabaoit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zabaoit/node-pro/issues"
  },
  "homepage": "https://github.com/zabaoit/node-pro#readme",
  "dependencies": {
    "@prisma/client": "6.3.0",
    "bcrypt": "5.1.1",
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1",
    "express-session": "1.18.1",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.12.0",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "uuid": "11.0.5",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/express-session": "1.18.1",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.7",
    "@types/passport": "1.0.17",
    "@types/passport-local": "1.0.38",
    "nodemon": "3.1.9",
    "prisma": "6.3.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  }
}
